#include <iostream>
#include<iomanip>
#include <math.h>

using namespace std;

int main() 
{
//1.定义变量 
    int n;
    cin >> n;
//2.定义数组，输入数据 
    int arr[n];
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
//3.输入目标数据 
    int target;
    cin >> target;
//4.寻找数据 
    int index = -1;
    for (int i = 0; i < n; i++) {
        if (arr[i] == target) {
            index = i;
            break;
        }
    }
//5.输出结果 
    cout << index << endl;

    return 0;
}
